echo "test 1"
./ft_ls -aRtrlz
ls -aRtrlz


echo "test 2"
./ft_ls -a--a
ls -a--a


echo "test 3"
./ft_ls "   -a"
ls "   -a"


echo "test 4"
./ft_ls - a
ls - a


echo "test 5"
./ft_ls --a
ls --a


echo "test 6"
./ft_ls auteur -a
ls auteur -a


echo "test 7"
./ft_ls -t auteur -- -l
ls -t auteur -- -l


echo "test 8"
./ft_ls - -l
ls - -l


echo "test 9"
./ft_ls -- -
ls -- -
